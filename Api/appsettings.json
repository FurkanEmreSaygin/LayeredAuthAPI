{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=FoundationAuthDb;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "JwtSettings": {
    "SecretKey": "Your SuperSecretKey Here", 
    "Issuer": "FoundationAuthAPI",
    "Audience": "FoundationAuthUsers",
    "ExpiresInDays": 60
  },
  "EmailSettings": {
    "SenderEmail": "your Email ",
    "SenderPassword": "your password",
    "SmtpHost": "your smtp host",
    "SmtpPort": 587,
    "EnableSsl": true
  }
}
